@using ASP.NET_Core_Introduction.ViewModels.Product
@model ProductViewModel;

@{
	ViewBag.Title = "Product #" + @Model.Id;
}

<div class="card">
	<div class="card-header">@ViewBag.Title</div>
	<div class="card-body">
		<h5 class="card-title">@Model.Name</h5>
		<p class="card-text">@Model.Price lv.</p>
	</div>
</div>
